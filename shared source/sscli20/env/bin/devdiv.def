PASS0_Pub:
!if defined(PASS0_PUBLISH) && "$(BUILD_PASS)" != "PASS1" && "$(BUILD_PASS)" != "PASS2"
    $(PUBLISH_CMD) -F <<
$(PASS0_PUBLISH:}=}
)
<<$(KEEPFILES)
!endif

PASS1_Pub: 
!if defined(PASS1_PUBLISH) && "$(BUILD_PASS)" != "PASS0" && "$(BUILD_PASS)" != "PASS2" && !defined(NOLIB)
    $(PUBLISH_CMD) -F <<
$(PASS1_PUBLISH:}=}
)
<<$(KEEPFILES)
