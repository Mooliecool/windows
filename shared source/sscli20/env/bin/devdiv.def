$(UMEXEFILES): $(UMLIBS) $(CRTLIBS) $(LINKLIBS) $(LIBC_LIB)

! ENDIF
!ENDIF

!if defined(BROWSER_INFO) && !defined(NO_BROWSER_FILE)

