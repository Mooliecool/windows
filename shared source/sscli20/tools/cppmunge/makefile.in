# ============================================================================
# File: makefile
# makefile for cppmunge
# ============================================================================

VPATH = ../..:
BIGENDIAN=@BIGENDIAN@
CFLAGS = @CPPMUNGE_CFLAGS@
TARGET_NAME = $(_NTTREE)/cppmunge
CC = @CC@

SOURCES=cppmunge.c

# Objects
OBJS = $(SOURCES:.c=.o)

#################
# Targets

all: $(TARGET_NAME)

$(TARGET_NAME) : $(OBJS)
	$(CC) -o $@ $(OBJS)

clean:
	rm -f $(OBJS)
	rm -f $(TARGET_NAME)

