CRT_INC_PATH=$(CORENV)\Crt\inc\i386

TARGETNAME=GenPalUniTable
TARGETPATH=.
TARGETTYPE=PROGRAM
COMPILER_WARNINGS=/W3 /WX
COR_C_FLAGS=-DUNICODE -D_UNICODE

